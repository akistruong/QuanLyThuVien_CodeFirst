@model IEnumerable<QLTV2._0.Models.Nxb>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Manxb)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Diachi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sdt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tennxb)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Manxb)
            </td>
             <td>
                @Html.DisplayFor(modelItem => item.Diachi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sdt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tennxb)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Manxb">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Manxb">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Manxb">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
