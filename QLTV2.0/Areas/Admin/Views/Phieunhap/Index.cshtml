@model IEnumerable<QLTV2._0.Models.Phieunhap>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                #ID
            </th>
            <th>
                Số lượng nhập
            </th>
            <th>
                Giá nhập
            </th>
            <th>
                Ngày tạo
            </th>
            <th>
                Ngày cập nhật
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdPhieunhap)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Soluongnhap)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gianhap)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Createdat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UpdatedAt)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Maphieunhap">Chỉnh sửa</a> |
                <a asp-action="Details" asp-route-id="@item.Maphieunhap">Chi Tiết</a> |
                <a asp-action="Delete" asp-route-id="@item.Maphieunhap">Xóa</a>
            </td>
        </tr>
}
    </tbody>
</table>
